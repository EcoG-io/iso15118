<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" 
xmlns="urn:din:70121:2012:MsgBody" 
xmlns:v2gci_d="urn:din:70121:2012:MsgDef"
xmlns:v2gci_t="urn:din:70121:2012:MsgDataTypes"
