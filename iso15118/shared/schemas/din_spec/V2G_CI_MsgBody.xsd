        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <!-- -->
  <!-- Service Detail -->
  <!-- -->
  <xs:element name="ServiceDetailReq" type="ServiceDetailReqType" substitutionGroup="v2gci_d:BodyElement"/>
  <xs:complexType name="ServiceDetailReqType">
    <xs:complexContent>
      <xs:extension base="v2gci_d:BodyBaseType">
