  <xs:element name="ServiceDiscoveryRes" type="ServiceDiscoveryResType" substitutionGroup="v2gci_d:BodyElement"/>
  <xs:complexType name="ServiceDiscoveryResType">
    <xs:complexContent>
      <xs:extension base="v2gci_d:BodyBaseType">
        <xs:sequence>
