[req]
prompt 					= no
distinguished_name		= ca_dn

[ca_dn]
commonName				= UKSWI123456789A
organizationName		= Switch
countryName				= UK
domainComponent			= MO

[ext]
basicConstraints		= critical,CA:false
keyUsage				= critical,digitalSignature,nonRepudiation,keyEncipherment,keyAgreement
subjectKeyIdentifier	= hash
authorityInfoAccess = OCSP;URI:http://ca.myown.com/, caIssuers;URI:http://ca.myown.com/Intermediate-CA.cer
