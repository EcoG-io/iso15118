[req]
prompt 					= no
distinguished_name		= ca_dn

[ca_dn]
commonName				= MORootCA
organizationName		= Switch
countryName				= UK
domainComponent			= MO

[ext]
basicConstraints		= critical,CA:true
keyUsage				= critical,keyCertSign,cRLSign
subjectKeyIdentifier	= hash

authorityInfoAccess = OCSP;URI:http://ca.myown.com/, caIssuers;URI:http://ca.myown.com/Intermediate-CA.cer
